import java.util.Scanner;

public class Solution {

    public static void main(String[] args) {
        Scanner psn=new Scanner(System.in);
        int a=psn.nextInt();
        Float b=psn.nextFloat();
        psn.nextLine();
        String c=psn.nextLine();
        System.out.printf("%s is %d years old and %.2f meters tall.",c,a,b);
    }
}
