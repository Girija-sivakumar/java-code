import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        int b=scan.nextInt();
        int e=scan.nextInt();
        int res=1;
        for(int i=1;i<=e;i++){
            res=res*b;
        }
        System.out.println(res);
        }
}
